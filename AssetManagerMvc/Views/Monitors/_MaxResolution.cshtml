@model AssetManagerMvc.Models.Monitor

<div class="form-group">
    @Html.LabelFor(model => model.MaxResolution, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10" style="float: left;">
        @Html.DropDownList("MaxResolution", null, htmlAttributes: new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.MaxResolution, "", new { @class = "text-danger" })
        <a id="typeLink" href="javascript:" onclick="changeToText('MaxResolution', 'typeLink'); return false;">New type?</a>
    </div>
</div>
<script src="@Url.Content("~/Scripts/script.js")" type="text/javascript"></script>