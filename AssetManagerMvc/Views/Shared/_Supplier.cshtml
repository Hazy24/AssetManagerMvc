@model AssetManagerMvc.Models.Asset

<div class="form-group">
    @Html.LabelFor(model => model.Supplier, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10" style="float: left;">
        @Html.DropDownList("Supplier", null, htmlAttributes: new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.Supplier, "", new { @class = "text-danger" })
        <a id="supplierLink" href="javascript:" onclick="changeToText('Supplier', 'supplierLink'); return false;">New supplier?</a>
    </div>
</div>

