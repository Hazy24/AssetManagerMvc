@model AssetManagerMvc.Models.UsePeriod

<div class="form-group">
    @Html.LabelFor(model => model.Function, htmlAttributes: new { @class = "control-label col-md-2" })
    <div class="col-md-10" style="float: left;">
        @Html.DropDownList("Function", null, htmlAttributes: new { @class = "form-control" })
        @Html.ValidationMessageFor(model => model.Function, "", new { @class = "text-danger" })
        <a id="functionLink" href="javascript:" onclick="changeToText('Function', 'functionLink'); return false;">New function?</a>
    </div>
</div>
<script src="@Url.Content("~/Scripts/script.js")" type="text/javascript"></script>
